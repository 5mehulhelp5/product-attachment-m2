<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="adminhtml_import_index"/>
    <body>
        <referenceContainer name="content">
            <block class="Mavenbird\ProductAttachment\Block\Adminhtml\Import" name="ImportEntityTypeHider" template="Mavenbird_ProductAttachment::files_import.phtml"/>
        </referenceContainer>
        <referenceContainer name="page.main.actions">
            <block class="Mavenbird\ProductAttachment\Block\Adminhtml\Steps" name="import-steps" template="Mavenbird_ProductAttachment::import_steps.phtml" before="-"/>
        </referenceContainer>
    </body>
</page>
