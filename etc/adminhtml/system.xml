<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="mavenbird" sortOrder="100" translate="label" class="tab-mavenbird-extensions">
            <label><![CDATA[Mavenbird Extensions]]></label>
        </tab>
        <section id="ProductAttachment"  showInDefault="1" showInWebsite="1" showInStore="1">
            <tab>mavenbird</tab>
            <label>Product Attachment</label>
            <resource>Mavenbird_ProductAttachment::config_Mavenbird_productattachment</resource>
            <group id="module"  showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Module</label>
                <field id="productattachment" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Enabled ProductAttachment</label>
                    <source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
                </field>
			</group>
        </section>
	</system>
</config>
